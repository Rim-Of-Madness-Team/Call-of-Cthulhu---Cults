  <!-- RACE DEFS -->
  <ThingDef ParentName="BaseCultHorrorRaceInit">
    <defName>Cults_FormlessSpawnRace</defName>
    <thingClass>Pawn</thingClass>
    <label>formless spawn</label>
    <tradeability>None</tradeability>
    <description>Deep beneath the earth, formless spawn rest in basins in Tsathoggua temples and act as guardians of the holy grounds. They are formed of goo capable of acidic digestion of matter, and thus are formidable foes with high resistance and high damage.</description>
    <statBases>
      <MoveSpeed>6.0</MoveSpeed>
      <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.05</ArmorRating_Sharp>
      <MeatAmount>0</MeatAmount>
      <CarryingCapacity>150</CarryingCapacity>
      <MarketValue>2800</MarketValue>
    </statBases>
    <tools>
      <li>
        <label>teeth</label>
        <capacities>
          <li>Cults_AcidTouch</li>
        </capacities>
        <power>20</power>
        <cooldownTime>2.5</cooldownTime>
        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
      </li>
      <li>
        <label>left claw</label>
        <capacities>
          <li>Cults_AcidTouch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>LeftHand</linkedBodyPartsGroup>
      </li>
      <li>
        <label>right claw</label>
        <capacities>
          <li>Cults_AcidTouch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>RightHand</linkedBodyPartsGroup>
      </li>
      <li>
        <label>tentacles</label>
        <capacities>
          <li>Cults_AcidTouch</li>
        </capacities>
        <power>10</power>
        <cooldownTime>1.5</cooldownTime>
        <linkedBodyPartsGroup>Cults_Tentacles</linkedBodyPartsGroup>
      </li>
    </tools>
    <techLevel>Animal</techLevel>
    <race>
      <trainability>Advanced</trainability>
      <wildness>0.95</wildness>
      <intelligence>Animal</intelligence>
      <thinkTreeMain>Animal</thinkTreeMain>
      <body>Cults_FormlessSpawnBody</body>
      <leatherDef>CH_Leather_FormlessSpawn</leatherDef>
      <baseBodySize>2.0</baseBodySize>
      <baseHealthScale>4.0</baseHealthScale>
      <lifeExpectancy>600</lifeExpectancy>
      <lifeStageAges>
        <li>
          <def>Cults_Spawn</def>
          <minAge>0</minAge>
        </li>
        <li>
          <def>Cults_FullyFormed</def>
          <minAge>5</minAge>
          <soundWounded>Pawn_Mech_Centipede_Wounded</soundWounded>
          <soundDeath>Pawn_Mech_Centipede_Death</soundDeath>
          <soundCall>Pawn_Mech_Centipede_Call</soundCall>
        </li>
        <li>
          <def>Cults_Ancient</def>
          <minAge>600</minAge>
        </li>
        <li>
          <def>Cults_Eldritch</def>
          <minAge>700</minAge>
        </li>
      </lifeStageAges>
      <nameGenerator>NamerAnimalGenericMale</nameGenerator>
      <nameGeneratorFemale>NamerAnimalGenericFemale</nameGeneratorFemale>
      <soundCallIntervalRange>
        <min>1000</min>
        <max>2000</max>
      </soundCallIntervalRange>
      <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
      <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
      <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
    </race>
    <comps>
      <li Class="CompProperties_SpawnerFilth">
        <filthDef>Cults_FilthBlackSlime</filthDef>
        <spawnCountOnSpawn>16</spawnCountOnSpawn>
        <spawnMtbHours>4</spawnMtbHours>
        <spawnRadius>1</spawnRadius>
      </li>
    </comps>
    <recipes>
      <li>Euthanize</li>
    </recipes>
  </ThingDef>
