<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>Cults_TransportByakhee</defName>
    <label>transport byakhee (hune form)</label>
    <description>A creature capable of carrying people, items, or animals through the vacuum of space by means of an organ known as the "hune." Can be used for sending gifts, transporting people or supplies, surprise attacks, or reinforcing battles.</description>
    <graphicData>
        <texPath>Pawn/Byakhee/Byakhee</texPath>
        <graphicClass>Graphic_Multi</graphicClass>
        <drawSize>4.5</drawSize>
        <shadowData>
        <volume>(0.6, 0.8, 0.6)</volume>
        </shadowData>
    </graphicData>
    <size>(1,1)</size>
    <rotatable>true</rotatable>
    <tickerType>Normal</tickerType>
    <altitudeLayer>BuildingOnTop</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>false</castEdgeShadows>
    <fillPercent>0.5</fillPercent>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>1600</WorkToBuild>
      <Flammability>0.5</Flammability>
    </statBases>
    <costList>
      <Steel>0</Steel>
    </costList>
    <comps>
      <li Class="CompProperties_Transporter">
        <restEffectiveness>0.8</restEffectiveness>
      </li>
      <li Class="CompProperties_Launchable">
        <skyfallerLeaving>DropPodLeaving</skyfallerLeaving>
      </li>
    </comps>
    <inspectorTabs>
      <li>ITab_ContentsTransporter</li>
    </inspectorTabs>
    <constructionSkillPrerequisite>6</constructionSkillPrerequisite>
    <uiIconScale>0.65</uiIconScale>
  </ThingDef>
  
</Defs>