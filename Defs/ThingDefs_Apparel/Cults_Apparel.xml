<Defs>

	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Apparel_CultistRobes</defName>
		<description>For those with knowledge.</description>
		<label>cultist robes</label>
		<graphicData>
			<texPath>Apparel/Display/CultistRobes</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>80</costStuffCount>
		<generateCommonality>0.01</generateCommonality>
