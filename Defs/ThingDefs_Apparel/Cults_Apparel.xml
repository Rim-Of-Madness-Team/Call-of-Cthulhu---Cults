			<SocialImpact>0.15</SocialImpact>
			<ResearchSpeed>0.05</ResearchSpeed>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
				<li>Torso</li>
				<li>Shoulders</li>
				<li>Arms</li>
				<li>Legs</li>
			</bodyPartGroups>
			<wornGraphicPath>Apparel/Bodywear_Clothing/CultistRobes/CultistRobes</wornGraphicPath>
			<layers>
				<li>Shell</li>
			</layers>

			<tags>
				<li>CallOfCthulhu_FrenziedCultist</li>
			</tags>
